[user]
	email = jvssxxi@gmail.com
	name = javalsai
[core]
	editor = $EDITOR
[safe]
	directory = /opt/WALC
	directory = /opt/better-discord
	directory = /home/javalsai/tmp/web-root
[gpg]
	format = ssh
[gpg "ssh"]
	allowedSignersFile = /home/javalsai/.ssh/allowed_signers
	defaultKeyCommand = sh -c 'echo key::$(local-ssh-key)'
[commit]
	gpgsign = true
[init]
	defaultBranch = master
[merge]
	conflictStyle = zdiff3
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	unstage = "restore --staged"
