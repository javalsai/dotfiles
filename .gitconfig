[user]
	email = jvssxxi@gmail.com
	name = javalsai
[core]
	editor = $EDITOR
	attributesfile = ~/.gitattributes
[safe]
	directory = /opt/WALC
	directory = /opt/better-discord
	directory = /home/javalsai/tmp/web-root
	directory = /var/www/tuxcord.net
	directory = /var/tuxcord
[gpg]
	format = ssh
[gpg "ssh"]
	allowedSignersFile = ~/.ssh/allowed_signers
	defaultKeyCommand = sh -c 'echo key::$(local-ssh-key)'
[commit]
	gpgsign = true
[init]
	defaultBranch = master
[merge]
	conflictStyle = zdiff3
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	unstage = "restore --staged"
[diff "exif"]
	textconv = exiftool
